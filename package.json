{
  "name": "@rangermauve/webrun",
  "version": "3.0.2",
  "description": "A custom module loader and global shim for Node to make it compatible with the browser.",
  "main": "src/index.js",
  "scripts": {
    "coverage": "node --experimental-vm-modules ./node_modules/.bin/istanbul cover tape src/test.js",
    "lint": "standard --fix 'src/**/*.js' 'bin/**/*.js'",
    "test": "node --experimental-vm-modules test/test.js",
    "prepublish": "git config core.autocrlf false && git rm --cached -r . && git reset --hard"
  },
  "bin": "bin/cli-runner.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RangerMauve/node-web-loader.git"
  },
  "keywords": [
    "esm",
    "ecmascript",
    "modules",
    "loader"
  ],
  "author": "rangermauve",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RangerMauve/node-web-loader/issues"
  },
  "homepage": "https://github.com/RangerMauve/node-web-loader#readme",
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "intercept-stdout": "^0.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "node": "^10.8.0",
    "standard": "^12.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "@beaker/dat-node": "github:beakerbrowser/dat-node",
    "@mattiasbuelens/web-streams-polyfill": "^0.2.0",
    "abab": "^2.0.0",
    "child-process-promise": "^2.2.1",
    "event-target": "^1.2.3",
    "fake-indexeddb": "^2.0.4",
    "filenamify-url": "^1.0.0",
    "form-data": "^2.3.2",
    "fs-extra": "^7.0.0",
    "ipfs": "git+ssh://git@github.com/ipfs/js-ipfs.git",
    "leveldown": "^5.0.0-0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "node-localstorage": "^1.3.1",
    "text-encoder": "0.0.3",
    "ws": "^6.0.0",
    "yargs": "^12.0.1"
  }
}
